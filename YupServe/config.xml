<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ntspl.yupserve" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>YupServe</name>
    <description>
        On demand service.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        NTSPL mobile application Team
    </author>
    <content src="index.html" />
    
    <!-- <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" /> -->
    <access origin="http://yupserve.com" />
    <access origin="https://yupserve.com" />
    <allow-navigation href="http://yupserve.com" />
    <allow-navigation href="https://yupserve.com" />
    <allow-navigation href="data:*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <access origin="*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="14" />
        <allow-intent href="market:*" />
        <icon qualifier="ldpi" src="res/icons/android/drawable-ldpi-icon.png" />
        <icon qualifier="mdpi" src="res/icons/android/drawable-mdpi-icon.png" />
        <icon qualifier="hdpi" src="res/icons/android/drawable-hdpi-icon.png" />
        <icon qualifier="xhdpi" src="res/icons/android/drawable-xhdpi-icon.png" />
        <icon qualifier="xxhdpi" src="res/icons/android/drawable-xxhdpi-icon.png" />
        <icon qualifier="xxxhdpi" src="res/icons/android/drawable-xxxhdpi-icon.png" />
        <splash qualifier="land-ldpi" src="res/screens/android/splash-port-ldpi.png" />
        <splash qualifier="land-mdpi" src="res/screens/android/splash-port-mdpi.png" />
        <splash qualifier="land-hdpi" src="res/screens/android/splash-port-hdpi.png" />
        <splash qualifier="land-xhdpi" src="res/screens/android/splash-port-xhdpi.png" />
        <splash qualifier="land-xxhdpi" src="res/screens/android/splash-port-xxhdpi.png" />
        <splash qualifier="land-xxxhdpi" src="res/screens/android/splash-port-xxxhdpi.png" />
        <splash qualifier="port-ldpi" src="res/screens/android/splash-port-ldpi.png" />
        <splash qualifier="port-mdpi" src="res/screens/android/splash-port-mdpi.png" />
        <splash qualifier="port-hdpi" src="res/screens/android/splash-port-hdpi.png" />
        <splash qualifier="port-xhdpi" src="res/screens/android/splash-port-xhdpi.png" />
        <splash qualifier="port-xxhdpi" src="res/screens/android/splash-port-xxhdpi.png" />
        <splash qualifier="port-xxxhdpi" src="res/screens/android/splash-port-xxxhdpi.png" />
        <preference name="AndroidLaunchMode" value="singleTop" />
        <preference name="KeepRunning" value="true" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>

    <preference name="DisallowOverscroll" value="true" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarBackgroundColor" value="##ff9800" />
    <engine name="android" spec="^7.1.4" />
    <engine name="browser" spec="~5.0.4" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-network-information" spec="^2.0.1" />
    <plugin name="cordova-plugin-fcm" spec="^2.1.2" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.2" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.7.1" />
    <plugin name="com.payumoney.sdkui.cordova" spec="^1.1.1" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="cordova-plugin-inappbrowser" spec="^3.0.0" />
</widget>
